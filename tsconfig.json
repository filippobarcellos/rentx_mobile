{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true
  },
  "baseUrl": "./src",
  "paths": {
    "@components/*": ["./components/*"],
    "@contexts/*": ["./contexts/*"],
    "@assets/*": ["./assets/*"],
    "@hooks/*": ["./hooks/*"],
    "@services/*": ["./services/*"],
    "@utils/*": ["./utils/*"]
  },
  "include": ["./src"]
}
